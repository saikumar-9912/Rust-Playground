fn main() {
    let some_number = Some(23);
    if let Some(value) = some_number {
        println!("Matched! The value is: {}", value);
    } else {
        println!("No match found.");
    }
    let another_number: Option<i32> = None;

    if let Some(value) = another_number {
        println!("Matched! The value is: {}", value);
    } else {
        println!("No match found in another_number.");
    }

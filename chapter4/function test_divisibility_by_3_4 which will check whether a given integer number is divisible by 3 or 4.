fn main() {
    let number = 12;
    let result = test_divisibility_by_3_4(number);
    println!("Result for {}: {}", number, result);
}
fn test_divisibility_by_3_4(num: i32) -> i32 {
    if num % 3 == 0 && num % 4 == 0 {
        0
    } else if num % 3 == 0 {
        1
    } else if num % 4 == 0 {
        2
    } else {
        -1
    }
}

fn main() {
    let int_value: i32 = 42;
    let float_value: f64 = int_value as f64;

    let char_value: u8 = 65;
    let character: char = char_value as char;

    println!("Integer value: {}", int_value);
    println!("After casting to float: {}", float_value);
    println!("U8 value: {}", char_value);
    println!("After casting to char: {}", character);
}
